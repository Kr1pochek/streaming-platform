{
  "name": "music",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "dev:server": "node --env-file-if-exists=.env server/index.js",
    "server": "node --env-file-if-exists=.env server/index.js",
    "audio:import": "node scripts/import-track-audio.mjs",
    "stream:hls": "node scripts/stream/generate-hls.mjs",
    "stream:verify": "node scripts/stream/verify-hls.mjs",
    "media:migrate:s3": "node --env-file-if-exists=.env scripts/stream/migrate-audio-to-s3.mjs",
    "db:migrate": "node --env-file-if-exists=.env scripts/db/migrate.mjs",
    "db:seed": "node --env-file-if-exists=.env scripts/db/seed.mjs",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test": "node --test --test-isolation=none \"tests/**/*.test.mjs\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.987.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "hls.js": "^1.6.15",
    "multer": "^2.0.2",
    "pg": "^8.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4"
  }
}
